{
    "name": "codemommy/webphp",
    "type": "project",
    "license": "Apache-2.0",
    "homepage": "http://www.codemommy.com",
    "description": "Web MVC framework of light and fast",
    "keywords": [
        "CodeMommy",
        "WebPHP",
        "Web",
        "PHP",
        "Framework",
        "Fast"
    ],
    "support": {
        "issues": "https://github.com/CodeMommy/WebPHP/issues",
        "source": "https://github.com/CodeMommy/WebPHP"
    },
    "authors": [
        {
            "name": "Candison November",
            "role": "Founder",
            "email": "kandisheng@163.com",
            "homepage": "http://www.kandisheng.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CodeMommy\\WebPHP\\": "system"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeMommy\\WebPHP\\Test\\": "test",
            "CodeMommy\\WebPHP\\Script\\": "script"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "CodeMommy/AutoloadPHP": "0.0.*",
        "CodeMommy/CachePHP": "0.0.*",
        "CodeMommy/ClientPHP": "0.0.*",
        "CodeMommy/ConfigPHP": "0.0.*",
        "CodeMommy/ConvertPHP": "0.0.*",
        "CodeMommy/CookiePHP": "0.0.*",
        "CodeMommy/CurlPHP": "0.0.*",
        "CodeMommy/DatabasePHP": "0.0.*",
        "CodeMommy/DateTimePHP": "0.0.*",
        "CodeMommy/DebugPHP": "0.0.*",
        "CodeMommy/FilePHP": "0.0.*",
        "CodeMommy/ImagePHP": "0.0.*",
        "CodeMommy/IsPHP": "0.0.*",
        "CodeMommy/LogPHP": "0.0.*",
        "CodeMommy/MailPHP": "0.0.*",
        "CodeMommy/RequestPHP": "0.0.*",
        "CodeMommy/ResponsePHP": "0.0.*",
        "CodeMommy/RoutePHP": "0.0.*",
        "CodeMommy/ServerPHP": "0.0.*",
        "CodeMommy/SessionPHP": "0.0.*",
        "CodeMommy/ViewPHP": "0.0.*",
        "symfony/yaml": "2.8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "<7.0.0",
        "phpmd/phpmd": "*",
        "squizlabs/php_codesniffer": "*",
        "satooshi/php-coveralls": "*",
        "CodeMommy/TaskPHP": "*"
    },
    "scripts": {
        "post-create-project-cmd": [
            "php script/Install.php 0.0.*",
            "composer update"
        ],
        "environment": [
            "composer -V",
            "php -v",
            "php -m"
        ],
        "update-project": [
            "git pull",
            "composer self-update",
            "composer update"
        ],
        "test": [
            "composer phpunit",
            "composer phpcbf",
            "composer phpcs",
            "composer phpmd"
        ],
        "phpmd": "\"vendor/bin/phpmd\" autoload.php,system,application,public,script,test,test_case text cleancode,codesize,controversial,design,naming,unusedcode",
        "phpcs": "\"vendor/bin/phpcs\" autoload.php system application public script test test_case --standard=PSR2",
        "phpcbf": "\"vendor/bin/phpcbf\" autoload.php system application public script test test_case --standard=PSR2",
        "phpunit": "CodeMommy\\WebPHP\\Script\\PHPUnit::start",
        "clean": "CodeMommy\\WebPHP\\Script\\Clean::start",
        "demo": "CodeMommy\\WebPHP\\Script\\Demo::start",
        "run": "CodeMommy\\WebPHP\\Script\\Run::start"
    }
}